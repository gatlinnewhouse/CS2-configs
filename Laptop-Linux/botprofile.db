//
// All profiles begin with this data and overwrite their own
//

Default
	Skill = 50
	Aggression = 50
	ReactionTime = 0.25				// reaction time in milliseconds. 230ms or 0.23 is standard human reaction time (0.3 default)
	AttackDelay = 0.20				// compensate for a bots lack of ping. 50ms is standard server ping (0 default)
	Teamwork = 75
	AimFocusInitial = 0.20			// initial focus spread in degrees (from desired center. 20 default)
	AimFocusDecay = 0.07			// how much focus shrinks per second (.25 = 25% of size after 1 sec. 0.7 default)
	AimFocusOffsetScale = 0.20		// controls accuracy when tracking to target (0 == perfect, should always be < 1)
	AimfocusInterval = 0.56			// how often focus is adjusted (smaller intervals means better movement tracking)
	WeaponPreference = CompWeps
	Cost = 0
	Difficulty = NORMAL
	VoicePitch = 100
	Skin = 0
	LookAngleMaxAccelNormal = 4000.0	// how quickly a bot will move their cross hair look at you (2000.0 default)
	LookAngleStiffnessNormal = 500.0	// how quickly a bot will lock onto you (100.0 default)
	LookAngleDampingNormal = 50.0		// dampen how much will the bot scan the area while not engaged (25.0 default)
	LookAngleMaxAccelAttacking = 5500.0 // how long a bot will keep an interest in their attacker (3000.0 default)
	LookAngleStiffnessAttacking = 525.0 // how soon after spotting will a bot attack (150.0 default)
	LookAngleDampingAttacking = 32.5	// dampen how much a bot will compensate for recoil (30.0 default)
End

